digraph {
	rankdir=LR
	i [label="" shape=point]
	i -> q0 [arrowsize=0.5]
	q0 [shape=circle]
	q1 [shape=circle]
	q2 [shape=doublecircle]
	q0 -> q1 [label=a arrowsize=0.5]
	q0 -> q2 [label=b arrowsize=0.5]
	q1 -> q1 [label=a arrowsize=0.5]
	q1 -> q2 [label=b arrowsize=0.5]
	q2 -> q2 [label=b arrowsize=0.5]
}
