digraph {
	rankdir=LR
	i [label="" shape=point]
	i -> q0 [arrowsize=0.5]
	q0 [shape=circle]
	q2 [shape=circle]
	q4 [shape=doublecircle]
	q6 [shape=circle]
	q0 -> q2 [label=0 arrowsize=0.5]
	q0 -> q6 [label=1 arrowsize=0.5]
	q2 -> q4 [label=0 arrowsize=0.5]
	q2 -> q6 [label=1 arrowsize=0.5]
	q4 -> q4 [label=0 arrowsize=0.5]
	q4 -> q4 [label=1 arrowsize=0.5]
	q6 -> q6 [label=0 arrowsize=0.5]
	q6 -> q6 [label=1 arrowsize=0.5]
}
